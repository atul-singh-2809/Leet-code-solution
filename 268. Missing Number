class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        int i=0;
        for(int j=0;j<nums.length;j++){
            if(i==nums[i])
            i++;

            else
            return i;
        }
        // if(i==nums.length)
        // return i;

        return i;
    }
}
