class Solution {
    public int singleNumber(int[] nums) {
        int i=0;
        Arrays.sort(nums);
        for(i=0;i<nums.length-2;i++){
            if(nums[i]!=nums[i+1]){
                return nums[i];
            }
            else{
                i++;
            }
           
    }
    return nums[i];
}}
